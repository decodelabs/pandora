{
    "name": "decodelabs/pandora",
    "description": "Potent PSR-11 depdency injection container",
    "type": "library",
    "keywords": [ "container", "psr-11", "dependency", "injection" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "psr/container": "^2",

        "decodelabs/archetype": "^0.3",
        "decodelabs/exceptional": "^0.5",
        "decodelabs/glitch-support": "^0.5",
        "decodelabs/slingshot": "^0.1.12"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "provide": {
        "psr/container-implementation": "1.1|2.0"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Pandora\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
