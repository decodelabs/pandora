{
    "name": "decodelabs/pandora",
    "description": "Potent PSR-11 depdency injection container",
    "type": "library",
    "keywords": ["container", "psr-11", "dependency", "injection"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",
        "psr/container": "^2",

        "decodelabs/glitch-support": "^0.4",
        "decodelabs/exceptional": "^0.4"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.6"
    },
    "provide": {
        "psr/container-implementation": "1.1|2.0"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Pandora\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    }
}
